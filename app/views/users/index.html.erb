<%= link_to "Create New User", new_user_path %>

<ul>
    <% @users.each do |user| %>
        <li>
            <%= user.name %>
            <% if current_user.admin? && user != current_user %>
                | <%= link_to "delete", user_path(user), method: :delete %>
            <% end %>
        </li>
    <% end %>
</ul>